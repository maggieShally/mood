/* components/picture/picture.wxss */

@import '../../common.wxss';

.picture-list {
  width: 100%;
  height: 300rpx;
  overflow: auto;
  white-space:nowrap;
}

.picture-item {
  display: inline-block;
  margin:0 35rpx 20rpx 0;
  width: 200rpx;
  height: 100%;
  overflow:hidden;
}

.picture-item image {
  width: 100%;
  height: 100%;
}